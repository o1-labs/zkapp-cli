    "eslint": "^8.7.0",
    "eslint-plugin-o1js": "^0.4.0",
    "prettier": "^2.3.2",
    "ts-jest": "^29.2.4",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "o1js": "^2.*"
  },
  "engines": {
    "node": ">=18.14.0"
  }
